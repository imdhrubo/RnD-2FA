@model TwoFactorAuth.Models.ManageViewModels.TwoFactorAuthenticationViewModel

@{
    ViewData["Title"] = "TwoFactorAuthenticationByAuthenticator";
}
<h4>@ViewData["Title"]</h4>


<a asp-controller="Manage" asp-action="SetupAuthenticator" class="btn btn-primary">Configure authenticator app</a>
<br />
<br />
<form asp-controller="Manage" asp-action="ResetAuthenticator" method="post">
    <button class="btn btn-primary">Reset authenticator key</button>
</form>


@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}

